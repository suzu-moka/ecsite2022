<!-- 各sectionをループで作成 -->
{% for topPhoto in goods %}
<section class="{{ topPhoto.url }}">
	<a href="./goods/{{topPhoto.url}}">
	<h2>{{ topPhoto.name }}</h2>
	<div class="mainimage">
		<img src="images/{{ topPhoto.itemphotos[0] }}" alt="{{ topPhotos.name }}メイン画像" />
	</div>
	</a>
</section>
{% endfor %}